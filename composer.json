{
	"name": "projectsend/projectsend",
	"description": "ProjectSend is an open source, clients-oriented, private file sharing web application.",
	"type": "project",
	"license": "GPL-3.0-or-later",
	"homepage": "https://www.projectsend.org/",
	"support": {
			"email": "contact@projectsend.org",
			"issues": "https://github.com/projectsend/projectsend/issues",
			"source": "https://github.com/projectsend/projectsend/"
	},
	"config": {
		"preferred-install": "dist"
	},
	"prefer-stable": true,
	"autoload": {
        "psr-4": {
            "ProjectSend\\": "includes/"
        },
        "classmap": ["includes"],
		"files": [
            "includes/helpers/functions.php",
            "includes/helpers/functions.actionslog.php",
            "includes/helpers/functions.assets.php",
            "includes/helpers/functions.categories.php",
            "includes/helpers/functions.forms.php",
            "includes/helpers/functions.groups.php",
            "includes/helpers/functions.i18n.php",
            "includes/helpers/functions.options.php",
            "includes/helpers/functions.public.php",
            "includes/helpers/functions.session.permissions.php",
            "includes/helpers/functions.templates.php",
            "includes/helpers/functions.usermeta.php",
            "includes/helpers/functions.updates.php"
		]
	},
	"require": {
        "php": ">=7.0",
        "claviska/simpleimage": "~3",
        "enshrined/svg-sanitize": "^0.15.0",
        "moxiecode/plupload": "3.1.5",
        "paragonie/random_compat": "^2.0",
        "phpmailer/phpmailer": "~6.1",
		"tamtamchik/simple-flash": "^2.0",
		"hybridauth/hybridauth": "^3.0",
		"monolog/monolog": "^2.8",
		"php-di/php-di": "^6.4",
		"league/route": "^5.1",
		"laminas/laminas-diactoros": "^2.17",
		"laminas/laminas-httphandlerrunner": "^2.2"
	}
}
